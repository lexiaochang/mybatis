<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chris.mybais.mapper.AutoIncrementMapper" >

    <insert id="insertData" parameterType="com.chris.mybais.po.AutoIncrementEntity" >
        insert into auto_increment (name, age)
        values (#{name,jdbcType=VARCHAR}, #{age,jdbcType=VARCHAR})
    </insert>


</mapper>